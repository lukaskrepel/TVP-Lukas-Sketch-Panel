Loop = 1
i = 0
WHILE Loop
	tv_clipenumid -1 i
 	ClipID = result
	IF CMP(ClipID,"none") == 1
		Loop = 0
	ELSE
		tv_ClipName ClipID
		ClipName = result
		IF Find(ClipName, "_" , 1) == 0
			ClipNumber = i + 1
			PostFixedFrame = addZeroesPostfix("_", i + 1, 3)
			tv_ClipName ClipID PostFixedFrame""ClipName
		END
	END
	i = i + 1
END

